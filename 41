import java.io.*;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
				Scanner cin = new Scanner(new BufferedInputStream(System.in));
				int[] arr = new int[3];
				for(int i=0;i<3;i++){
					arr[i] = cin.nextInt();
				}
				Arrays.sort(arr);
				for(int i=0;i<3;i++)
					System.out.print(arr[i]+" ");
				
	}	 
}
