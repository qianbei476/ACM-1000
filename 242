import java.io.*;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
				Scanner cin = new Scanner(new BufferedInputStream(System.in));				
				while(cin.hasNext()){
					double R = cin.nextDouble();
					System.out.println( Math.round(((3.1415926)*Math.pow(R, 3)*4)/3));
				}
	}	 
}
