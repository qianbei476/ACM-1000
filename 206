import java.io.*;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
				Scanner cin = new Scanner(new BufferedInputStream(System.in));
				while(cin.hasNext()){
					long x = cin.nextInt();
					long y = cin.nextInt();
					long sum = 0;
					for(int i=1;i<=x;i++)
						for(int j=1;j<=y;j++){
							sum+=(x-i+1)*(y-j+1);
						}

					System.out.println(sum);
				}
	}
}
