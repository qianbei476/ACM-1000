import java.io.*;
import java.util.*;

public class Main {
	
	
	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		while(cin.hasNext()){
			int n = cin.nextInt();
			for(int i=0;i<n;i++){
				for(int j=0;j<n;j++){
					System.out.print("*");
				}
				System.out.println("");
			}
		}
	}
}
