import java.io.*;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		Scanner cin = new Scanner(new BufferedInputStream(System.in));
		int t = cin.nextInt();
		int m = t;
		while(t>=2 && t<30 && m>0){
			int n = cin.nextInt();
			if(n>=2&&n<=10000){
			for(int i=1;i<=n-1;i+=2)
			System.out.print(i+" ");
			System.out.println("");
			for(int i=2;i<=n;i+=2)
			System.out.print(i+" ");
			m--;
			}
		}
				
	}	 
}
