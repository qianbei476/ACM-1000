import java.io.*;
import java.util.*;

public class Main {

	public static void main(String[] args) {
				Scanner cin = new Scanner(new BufferedInputStream(System.in));
				int N = cin.nextInt();
				while(N>0){
					int num = 0;
					int M = cin.nextInt();
					char[] arr = Integer.toBinaryString(M).toCharArray();
					for(int i=0;i<arr.length;i++){
						if(arr[i]=='1')
							num++;
					}
					System.out.println(num);
				N--;
				}
		}	 
} 




